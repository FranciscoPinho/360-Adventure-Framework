{
  "a-videosphere": {
    "id": "scene1",
    "src": "#my-video",
    "video-player": {
      "videoVolume":0
    },
    "video-looper": "",
    "music-change-emitter": {
      "newsource": "#music",
      "loop": "true",
      "volume": 0.1,
      "cacheDuration": "true"
    },
    "scripted-audio-player": {
      "src":"#beginAudio",
      "autoplay":true,
      "removeSelfOnEnd":true,
      "volume":0.1
    },
    "children": [
      {
        "a-sphere": {
          "id":"sphere1",
          "class": "inter",
          "scripted-invisibility":"",
          "navigation-event-emitter": {
            "firstdestination": "cutscenes/cutscene2.json",
            "destination": "playable-scenes/scene2.json"
          },
          "position": "5 -2 0.5",
          "geometry": {
            "primitive": "sphere",
            "radius": "0.3"
          }
        }
      },
      {
        "a-plane":{
          "id":"invisibleplane",
          "class":"inter",
          "geometry": {
            "primitive": "plane"
          },
          "position": "-12 0 -9.974",
          "rotation": "0 40 0",
          "scale": "1.5 2 1.5",
          "invisible-in-vr":"",
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "nofeedback",
            "hoverIcon": "#examineIcon"
          },
          "dialogue":{
            "dialogueTreeURL":"dialogue/dialogues.json",
            "spawn":{
              "location":"look",
              "zDistance":"-5",
              "opacity":0.6
            },
            "sfx":{
              "advanceSfx":"#hover",
              "advanceVolume":1,
              "spawnSfx":"#hover",
              "spawnVolume":1
            },
            "examinableObject":true,
            "examinedIcon":"#examinedIcon"
          }
        },
        "a-image": {
          "id": "person",
          "class": "inter",
          "src": "#personImg",
          "position": "8 -1.4 -4",
          "rotation": "0 -90 0",
          "scale": "1.5 1.5 1.5",
          "material":{
            "shader":"flat",
            "alphaTest":0.5
          },
          "dialogue":{
            "dialogueTreeURL":"dialogue/dialogues.json",
            "dialogueTreeName":"RPGTree",
            "spawn":{
              "location":"look",
              "zDistance":"-5",
              "opacity":0.6
            },
            "sfx":{
              "advanceSfx":"#hover",
              "advanceVolume":1,
              "spawnSfx":"#hover",
              "spawnVolume":1,
              "hoverChoiceSfx":"#hover",
              "hoverChoiceVolume":1,
              "choiceSfx":"#choiceSfx",
              "choiceVolume":0.3
            },
            "choiceIcon":"#chooseIcon",
            "previouslyChosenIcon":"#previouslyChosenIcon",
            "examinableObject":true,
            "pauseBackgroundSong":true,
            "examinedIcon":"#examinedIcon",
            "removeSelfOnEnd":false,
            "autoplay":false
          },
          "use-target":{
            "stimulus":[
              {
                "targeted_by": "flame",
                "dialogue":{
                  "dialogueTreeURL":"dialogue/dialogues.json",
                  "dialogueTreeName":"burnthisman",
                  "spawn":{
                    "location":"look",
                    "zDistance":"-5",
                    "opacity":0.6
                  },
                  "sfx":{
                    "advanceSfx":"#hover",
                    "advanceVolume":1,
                    "spawnSfx":"#hover",
                    "spawnVolume":1
                  },
                  "removeSelfOnEnd":true,
                  "autoplay":true
                }
              },
              {
                "targeted_by": "deadlydeer",
                "newFlag": "dead",
                "inventoryData": {
                  "originalID":"person",
                  "iconSrc": "#deadpersonImg",
                  "iconID": "deadperson",
                  "iconDesc":"Dead Agent Smith: This guy is dead, congratulations terrorist..."
                }
              },
              {
                "targeted_by":["deer1Icon","deer2Icon"],
                "newFlag": "deerman",
                "id":"deerman",
                "src": "#deermanImg",
                "scale":"5 5 5",
                "hoverable": {
                  "sfx":{
                    "sfxSrc": "#hover",
                    "volume": 1
                  },
                  "feedback": "color",
                  "itemOnly":true
                },
                "sfxSrc":"#matchlit",
                "volume":"0.5",
                "use-target":{
                  "stimulus":[
                    {
                      "targeted_by":["deer1Icon","deer2Icon"],
                      "newFlag": "deadperson",
                      "id":"deadperson",
                      "src": "#deadpersonImg",
                      "scale":"3 3 3"
                    }
                  ]
                }
              }
            ]
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#examineIcon"
          }
        }
      },
      {
        "a-image": {
          "id": "cutscene",
          "class": "inter",
          "src": "#clickToCutsceneImg",
          "position": "10 5 -5",
          "scale": "2 2 2",
          "rotation":"0 100 0",
          "material":{
            "shader":"flat"
          },
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#cutsceneIcon",
              "iconID": "cutsceneIcon",
              "iconDesc":"Cutscene?: A completely useless item, enjoy"
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer1",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "20 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer1Icon",
              "iconDesc":"Deer: I wonder what happens when two deers join forces..."
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer2",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "53 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer2Icon",
              "iconDesc":"Deer: I wonder what happens when two deers join forces..."
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      }
    ]
  },
  "addToInventory":[
    {"iconID": "icecube", "iconSrc": "#icecubeIcon", "iconDesc":"Ice Cube: An ice cube"},
    {"iconID": "flame", "iconSrc": "#flameIcon", "iconDesc":"Flame: A fiery flame, hot enough to melt stuff"},
    {"iconID": "flame", "iconSrc": "#flameIcon", "iconDesc":"Flame: A fiery flame, hot enough to melt stuff"},
    {"iconID": "oil", "iconSrc": "#oilIcon", "iconDesc":"Oil: Stains stuff black, disgusting"}
  ],
  "combinations": [
    {
      "mix":["deer1Icon","deer2Icon"],
      "result":{
         "iconSrc":"#deadlydeerIcon",
         "iconID":"deadlydeer",
         "iconDesc":"Deadliest Deer: Use it to kill any living being"
      }
    },
    {
      "mix":["icecube","flame"],
      "result":{
         "iconSrc":"#halficecubeIcon",
         "iconID":"halficecube",
         "iconDesc":"Half Melted Cube: Half ice, half water",
         "sfxSrc":"#matchlit",
         "volume":"0.5"
      }
    },
    {
      "mix":["halficecube","flame"],
      "result":{
         "iconSrc":"#puddleIcon",
         "iconID":"puddle",
         "iconDesc":"Puddle: It's a water puddle",
         "sfxSrc":"#matchlit",
         "volume":"0.5"
      }
    },
    {
      "mix":["puddle","oil"],
      "result":{
         "iconSrc":"#blackPuddleIcon",
         "iconID":"blackpuddle",
         "iconDesc":"Oily Puddle: It's a goddamn water puddle but with oil",
         "sfxSrc":"#oilsfx",
         "volume":"1"
      }
    }
  ],
  "transitions": [
    {
      "transitionID":"audioOnExamine",
      "on":[
        {"invisibleplane":"examined"}
      ],
      "playAudio":{
        "src":"#clickAudio",
        "autoplay":true,
        "exclusive":true,
        "removeSelfOnEnd":true,
        "volume":0.5
      }
    },
    {
      "transitionID":"deermandied",
      "on":[
        {"deerman":"deadperson"}
      ],
      "goToDialogue":{
        "dialogueTreeURL":"dialogue/dialogues.json",
        "dialogueTreeName":"remorsechoice",
        "spawn":{
          "location":"look",
          "zDistance":"-5",
          "opacity":0.6
        },
        "sfx":{
          "advanceSfx":"#hover",
          "advanceVolume":1,
          "spawnSfx":"#hover",
          "spawnVolume":1,
          "hoverChoiceSfx":"#hover",
          "hoverChoiceVolume":1,
          "choiceSfx":"#choiceSfx",
          "choiceVolume":1
        },
        "removeSelfOnEnd":true,
        "autoplay":true,
        "choiceIcon":"#chooseIcon"
      }
    },
    {
      "transitionID":"badend",
      "on":[ 
        {
          "person": "dead",
          "shownoremorse": "chosen",
          "blackpuddle": "inInventory"
        },
        {
          "deerman": "deadperson",
          "shownoremorse": "chosen",
          "blackpuddle": "inInventory"
        }
      ],
      "goToDestination": "cutscenes/cutscene3.json"
    },
    {
      "transitionID":"afterPickCutscene",
      "on":[
        {"cutscene":"picked"}
      ],
      "goToDestination":"cutscenes/cutscene1.json"
    },
    {
      "transitionID":"personDiedFromDeadlyDeer",
      "on":[
        {
          "person":"dead"
        }
      ],
      "goToDialogue":{
        "dialogueTreeURL":"dialogue/dialogues.json",
        "dialogueTreeName":"remorsechoice",
        "spawn":{
          "location":"look",
          "zDistance":"-5",
          "opacity":0.6
        },
        "sfx":{
          "advanceSfx":"#hover",
          "advanceVolume":1,
          "spawnSfx":"#hover",
          "spawnVolume":1,
          "hoverChoiceSfx":"#hover",
          "hoverChoiceVolume":1,
          "choiceSfx":"#choiceSfx",
          "choiceVolume":0.3
        },
        "removeSelfOnEnd":true,
        "autoplay":true,
        "choiceIcon":"#chooseIcon"
      }
    },
    {
      "transitionID":"deerpicked",
      "on":[
        {"deer1":"picked"},
        {"deer2":"picked"}
      ],
      "goToDialogue":{
        "dialogueTreeURL":"dialogue/dialogues.json",
        "dialogueTreeName":"deerpickup",
        "spawn":{
          "location":"look",
          "zDistance":"-5",
          "opacity":0.6
        },
        "sfx":{
          "advanceSfx":"#hover",
          "advanceVolume":1,
          "spawnSfx":"#hover",
          "spawnVolume":1,
          "hoverChoiceSfx":"#hover",
          "hoverChoiceVolume":1,
          "choiceSfx":"#pickup",
          "choiceVolume":1
        },
        "removeSelfOnEnd":true,
        "autoplay":true
      }
    },
    {
      "transitionID":"deerSadness",
      "on":[
        {
          "deer1":"picked",
          "deer2":"picked"
        }
      ],
      "injectFlatVideo":{
          "id":"2dvid",
          "src":"#flatvid",
          "video-player":{
            "flatCutscene":true,
            "pauseBackgroundSong":true,
            "removeEntityOnEnd":true
          },
          "zDistance":-8,
          "width":8,
          "height":4.5,
          "position":"look"
      }
    },
    {
      "transitionID":"sphereVisible",
      "on":[
        {
          "deer1":"picked"
        },
        {
          "deer2":"picked"
        }
      ],
      "makeVisible":"sphere1"
    },
    {
      "transitionID":"dialogueDisplay",
      "on":[
        {
          "person":"deerman"
        }
      ],
      "goToDialogue":{
        "dialogueTreeURL":"dialogue/dialogues.json",
        "dialogueTreeName":"deermanTransformed",
        "spawn":{
          "location":"look",
          "zDistance":"-5",
          "opacity":0.6
        },
        "sfx":{
          "advanceSfx":"#hover",
          "advanceVolume":1,
          "spawnSfx":"#hover",
          "spawnVolume":1
        },
        "autoplay":true,
        "removeSelfOnEnd":true
      }
    },
    {
      "transitionID":"changeBackgroundSrc",
      "on":[
        {
          "2dvid":"seen"
        }
      ],
      "changeBackgroundSrc":{
        "newSrc":"#my-video2",
        "playOnce":"true",
        "cutscene":"false"
      }
    },
    {
      "transitionID":"audioTransition",
      "on":[
        {
          "sphere1":"visited",
          "cutscene2":"seen"
        }
      ],
      "playAudio":{
        "src":"general-voiceover/invisible-fabric.wav",
        "autoplay":true,
        "exclusive":true,
        "removeSelfOnEnd":true,
        "volume":0.2
      }
    },
    {
      "transitionID":"goodend",
      "on":[
        {
          "person": ["deerman",""],
          "cutscene1": "seen"
        },
        {
          "person": "dead",
          "showremorse": "chosen",
          "activeBackgroundID":"scene2"
        }
      ],
      "newURL": "goodending.html",
      "clearInventory":true
    }
  ]
}
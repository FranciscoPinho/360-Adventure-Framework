{
  "a-videosphere": {
    "id": "scene1",
    "src": "#my-video",
    "video-player": "",
    "video-looper": "",
    "music-change-emitter": {
      "newsource": "#music",
      "loop": "true",
      "volume": 0.05,
      "cacheDuration": "true"
    },
    "children": [
      {
        "a-sphere": {
          "id":"sphere1",
          "class": "inter",
          "navigation-event-emitter": {
            "firstdestination": "scenes/cutscene2.json",
            "destination": "scenes/scene2.json"
          },
          "position": "5 -2 0.5",
          "geometry": {
            "primitive": "sphere",
            "radius": "0.3"
          }
        }
      },
      {
        "a-image": {
          "id": "person",
          "class": "inter",
          "src": "#person",
          "position": "8 -1.4 -4",
          "rotation": "0 -90 0",
          "scale": "1.5 1.5 1.5",
          "use-target":{
            "stimulus":[
              {
                "targeted_by": "deadlydeer",
                "newFlag": "dead",
                "inventoryData": {
                  "iconSrc": "#deadperson",
                  "iconID": "deadperson"
                }
              },
              {
                "targeted_by":["deer1","deer2"],
                "newFlag": "deerman",
                "id":"deerman",
                "src": "#deerman",
                "scale":"5 5 5",
                "use-target":{
                  "stimulus":[
                    {
                      "targeted_by":["deer1","deer2"],
                      "newFlag": "deadperson",
                      "id":"deadperson",
                      "src": "#deadperson",
                      "scale":"3 3 3"
                    }
                  ]
                }
              }
            ]
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "itemOnly": "true"
          }
        }
      },
      {
        "a-image": {
          "id": "cutscene",
          "class": "inter",
          "src": "#clickToCutscene",
          "position": "10 5 -5",
          "scale": "2 2 2",
          "rotation":"0 100 0",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#cutsceneIcon",
              "iconID": "cutscene"
            },
            "afterPickCutscene": "scenes/cutscene1.json"
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer1",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "20 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 0.05
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer1"
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer2",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "53 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 0.05
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer2"
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      }
    ]
  },
  "addToInventory":[
    {"iconID": "icecube", "iconSrc": "#icecubeIcon"},
    {"iconID": "flame", "iconSrc": "#flameIcon"},
    {"iconID": "flame", "iconSrc": "#flameIcon"},
    {"iconID": "oil", "iconSrc": "#oilIcon"}
  ],
  "combinations": [
    {
      "mix":["deer1","deer2"],
      "result":{
         "iconSrc":"#deadlydeerIcon",
         "iconID":"deadlydeer"
      }
    },
    {
      "mix":["icecube","flame"],
      "result":{
         "iconSrc":"#halficecubeIcon",
         "iconID":"halficecube"
      }
    },
    {
      "mix":["halficecube","flame"],
      "result":{
         "iconSrc":"#puddleIcon",
         "iconID":"puddle"
      }
    },
    {
      "mix":["puddle","oil"],
      "result":{
         "iconSrc":"#blackPuddleIcon",
         "iconID":"blackpuddle"
      }
    }
  ],
  "transitions": [
    {
      "on": {
        "person": "dead",
        "blackPuddle": "created"
      },
      "gotoCutscene": "scenes/cutscene3.json",
      "carryInventory":false
    },
    {
      "on": {
        "person": ["","deerman"],
        "cutscene1": "seen"
      },
      "gotoURL": "goodending.html",
      "carryInventory":true
    }
  ]
}
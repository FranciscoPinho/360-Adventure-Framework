{
  "a-videosphere": {
    "id": "scene1",
    "src": "#my-video",
    "video-player": "",
    "video-looper": "",
    "music-change-emitter": {
      "newsource": "#music",
      "loop": "true",
      "volume": 0.1,
      "cacheDuration": "true"
    },
    "scripted-audio-player": {
      "src":"#beginAudio",
      "autoplay":true,
      "removeSelfOnEnd":true,
      "volume":0.03
    },
    "children": [
      {
        "a-sphere": {
          "id":"sphere1",
          "class": "inter",
          "scripted-invisibility":"",
          "navigation-event-emitter": {
            "firstdestination": "scenes/cutscene2.json",
            "destination": "scenes/scene2.json"
          },
          "position": "5 -2 0.5",
          "geometry": {
            "primitive": "sphere",
            "radius": "0.3"
          }
        }
      },
      {
        "a-plane":{
          "id":"invisibleplane",
          "class":"inter",
          "geometry": {
            "primitive": "plane"
          },
          "position": "-12 0 -9.974",
          "rotation": "0 40 0",
          "scale": "1.5 2 1.5",
          "invisible-in-vr":"",
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "nofeedback",
            "hoverIcon": "#examineIcon"
          },
          "dialogue":{
            "dialogueTreeURL":"dialogue/dialogue4.json",
            "spawn":{
              "location":"look",
              "zDistance":"-5",
              "opacity":0.6
            },
            "sfx":{
              "advanceSfx":"#hover",
              "advanceVolume":1,
              "spawnSfx":"#hover",
              "spawnVolume":1
            },
            "examinableObject":true,
            "examinedIcon":"#examinedIcon"
          }
        },
        "a-image": {
          "id": "person",
          "class": "inter",
          "src": "#personImg",
          "position": "8 -1.4 -4",
          "rotation": "0 -90 0",
          "scale": "1.5 1.5 1.5",
          "material":{
            "shader":"flat",
            "alphaTest":0.5
          },
          "dialogue":{
            "dialogueTreeURL":"dialogue/dialogue1.json",
            "spawn":{
              "location":"look",
              "zDistance":"-5",
              "opacity":0.6
            },
            "sfx":{
              "advanceSfx":"#hover",
              "advanceVolume":1,
              "spawnSfx":"#hover",
              "spawnVolume":1,
              "hoverChoiceSfx":"#hover",
              "hoverChoiceVolume":1,
              "choiceSfx":"#choiceSfx",
              "choiceVolume":0.3
            },
            "choiceIcon":"#chooseIcon",
            "previouslyChosenIcon":"#previouslyChosenIcon",
            "examinableObject":true,
            "pauseBackgroundSong":true,
            "examinedIcon":"#examinedIcon",
            "removeSelfOnEnd":false,
            "autoplay":false
          },
          "use-target":{
            "stimulus":[
              {
                "targeted_by": "flame",
                "dialogue":{
                  "dialogueTreeURL":"dialogue/dialogue2.json",
                  "spawn":{
                    "location":"look",
                    "zDistance":"-5",
                    "opacity":0.6
                  },
                  "sfx":{
                    "advanceSfx":"#hover",
                    "advanceVolume":1,
                    "spawnSfx":"#hover",
                    "spawnVolume":1
                  },
                  "removeSelfOnEnd":true,
                  "autoplay":true
                }
              },
              {
                "targeted_by": "deadlydeer",
                "newFlag": "dead",
                "inventoryData": {
                  "originalID":"person",
                  "iconSrc": "#deadpersonImg",
                  "iconID": "deadperson",
                  "iconDesc":"Dead Agent Smith: This guy is dead, congratulations terrorist..."
                },
                "afterStimulusDialogue":{
                  "dialogueTreeURL":"dialogue/choice1.json",
                  "spawn":{
                    "location":"look",
                    "zDistance":"-5",
                    "opacity":0.6
                  },
                  "sfx":{
                    "advanceSfx":"#hover",
                    "advanceVolume":1,
                    "spawnSfx":"#hover",
                    "spawnVolume":1,
                    "hoverChoiceSfx":"#hover",
                    "hoverChoiceVolume":1,
                    "choiceSfx":"#choiceSfx",
                    "choiceVolume":0.3
                  },
                  "removeSelfOnEnd":true,
                  "autoplay":true,
                  "choiceIcon":"#chooseIcon"
                }
              },
              {
                "targeted_by":["deer1Icon","deer2Icon"],
                "newFlag": "deerman",
                "id":"deerman",
                "src": "#deermanImg",
                "scale":"5 5 5",
                "hoverable": {
                  "sfx":{
                    "sfxSrc": "#hover",
                    "volume": 1
                  },
                  "feedback": "color",
                  "itemOnly":true
                },
                "sfxSrc":"#matchlit",
                "volume":"0.5",
                "use-target":{
                  "stimulus":[
                    {
                      "targeted_by":["deer1Icon","deer2Icon"],
                      "newFlag": "deadperson",
                      "id":"deadperson",
                      "src": "#deadpersonImg",
                      "scale":"3 3 3",
                      "afterStimulusDialogue":{
                        "dialogueTreeURL":"dialogue/choice1.json",
                        "spawn":{
                          "location":"look",
                          "zDistance":"-5",
                          "opacity":0.6
                        },
                        "sfx":{
                          "advanceSfx":"#hover",
                          "advanceVolume":1,
                          "spawnSfx":"#hover",
                          "spawnVolume":1,
                          "hoverChoiceSfx":"#hover",
                          "hoverChoiceVolume":1,
                          "choiceSfx":"#choiceSfx",
                          "choiceVolume":1
                        },
                        "removeSelfOnEnd":true,
                        "autoplay":true,
                        "choiceIcon":"#chooseIcon"
                      }
                    }
                  ]
                }
              }
            ]
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#examineIcon"
          }
        }
      },
      {
        "a-image": {
          "id": "cutscene",
          "class": "inter",
          "src": "#clickToCutsceneImg",
          "position": "10 5 -5",
          "scale": "2 2 2",
          "rotation":"0 100 0",
          "material":{
            "shader":"flat"
          },
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#cutsceneIcon",
              "iconID": "cutsceneIcon",
              "iconDesc":"Cutscene?: A completely useless item, enjoy"
            },
            "afterPickCutscene": "scenes/cutscene1.json"
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer1",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "20 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer1Icon",
              "iconDesc":"Deer: I wonder what happens when two deers join forces..."
            },
            "afterPickDialogue":{
              "dialogueTreeURL":"dialogue/dialogue3.json",
              "spawn":{
                "location":"look",
                "zDistance":"-5",
                "opacity":0.6
              },
              "sfx":{
                "advanceSfx":"#hover",
                "advanceVolume":1,
                "spawnSfx":"#hover",
                "spawnVolume":1,
                "hoverChoiceSfx":"#hover",
                "hoverChoiceVolume":1,
                "choiceSfx":"#pickup",
                "choiceVolume":1
              },
              "removeSelfOnEnd":true,
              "autoplay":true
            }
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      },
      {
        "a-entity": {
          "id": "deer2",
          "class": "inter",
          "gltf-model": "#deer",
          "position": "53 -5.5 -12",
          "scale": "0.01 0.01 0.01",
          "rotation": "-0.19 110 0.005",
          "pickable": {
            "sfx":{
              "sfxSrc": "#pickup",
              "volume": 1
            },
            "inventoryData": {
              "iconSrc": "#deerIcon",
              "iconID": "deer2Icon",
              "iconDesc":"Deer: I wonder what happens when two deers join forces..."
            }
          },
          "scripted-audio-player": {
            "src":"#clickAudio",
            "volume":0.03
          },
          "hoverable": {
            "sfx":{
              "sfxSrc": "#hover",
              "volume": 1
            },
            "feedback": "color",
            "hoverIcon": "#pickIcon"
          }
        }
      }
    ]
  },
  "addToInventory":[
    {"iconID": "icecube", "iconSrc": "#icecubeIcon", "iconDesc":"Ice Cube: An ice cube"},
    {"iconID": "flame", "iconSrc": "#flameIcon", "iconDesc":"Flame: A fiery flame, hot enough to melt stuff"},
    {"iconID": "flame", "iconSrc": "#flameIcon", "iconDesc":"Flame: A fiery flame, hot enough to melt stuff"},
    {"iconID": "oil", "iconSrc": "#oilIcon", "iconDesc":"Oil: Stains stuff black, disgusting"}
  ],
  "combinations": [
    {
      "mix":["deer1Icon","deer2Icon"],
      "result":{
         "iconSrc":"#deadlydeerIcon",
         "iconID":"deadlydeer",
         "iconDesc":"Deadliest Deer: Use it to kill any living being"
      }
    },
    {
      "mix":["icecube","flame"],
      "result":{
         "iconSrc":"#halficecubeIcon",
         "iconID":"halficecube",
         "iconDesc":"Half Melted Cube: Half ice, half water",
         "sfxSrc":"#matchlit",
         "volume":"0.5"
      }
    },
    {
      "mix":["halficecube","flame"],
      "result":{
         "iconSrc":"#puddleIcon",
         "iconID":"puddle",
         "iconDesc":"Puddle: It's a water puddle",
         "sfxSrc":"#matchlit",
         "volume":"0.5"
      }
    },
    {
      "mix":["puddle","oil"],
      "result":{
         "iconSrc":"#blackPuddleIcon",
         "iconID":"blackpuddle",
         "iconDesc":"Oily Puddle: It's a goddamn water puddle but with oil",
         "sfxSrc":"#oilsfx",
         "volume":"1"
      }
    }
  ],
  "transitions": [
    {
      "transitionID":"badend",
      "on":[ 
        {
          "person": "dead",
          "shownoremorse": "chosen",
          "blackpuddle": "inInventory"
        },
        {
          "deerman": "deadperson",
          "shownoremorse": "chosen",
          "blackpuddle": "inInventory"
        }
      ],
      "goToDestination": "scenes/cutscene3.json",
      "clearInventory":false
    },
    {
      "transitionID":"goodend",
      "on":[
        {
          "person": ["deerman",""],
          "cutscene1": "seen"
        },
        {
          "person": "dead",
          "showremorse": "chosen",
          "activeBackgroundID":"scene2"
        }
      ],
      "newURL": "goodending.html",
      "clearInventory":true
    },
    {
      "transitionID":"sphereVisible",
      "on":[
        {
          "deer1":"picked"
        },
        {
          "deer2":"picked"
        }
      ],
      "makeVisible":"sphere1"
    },
    {
      "transitionID":"dialogueDisplay",
      "on":[
        {
          "person":"deerman"
        }
      ],
      "goToDialogue":{
        "dialogueTreeURL":"dialogue/dialogue4.json",
        "dialogueTreeName":"deermanTransformed",
        "spawn":{
          "location":"look",
          "zDistance":"-5",
          "opacity":0.6
        },
        "sfx":{
          "advanceSfx":"#hover",
          "advanceVolume":1,
          "spawnSfx":"#hover",
          "spawnVolume":1
        },
        "autoplay":true,
        "removeSelfOnEnd":true
      }
    },
    {
      "transitionID":"audioTransition",
      "on":[
        {
          "sphere1":"visited",
          "cutscene2":"seen"
        }
      ],
      "playAudio":{
        "src":"general-voiceover/invisible-fabric.wav",
        "autoplay":true,
        "exclusive":true,
        "removeSelfOnEnd":true,
        "volume":0.2
      }
    }
  ]
}